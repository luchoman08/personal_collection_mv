<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Usuarios" table="usuarios">
    <unique-constraints>
      <unique-constraint name="usuarios_num_identificacion_key" columns="num_identificacion"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="usuarios_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="numIdentificacion" type="string" column="num_identificacion" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombres" type="string" column="nombres" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="correoElectronico" type="string" column="correo_electronico" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
